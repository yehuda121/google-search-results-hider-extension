{
  "manifest_version": 3,
  "name": "Search Results Hider",
  "version": "1.1",
  "description": "Adds Hide buttons to Google search results for the current search only.",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*",
        "https://www.google.co.il/search*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
